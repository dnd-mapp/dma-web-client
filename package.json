{
    "name": "dma-web-client",
    "version": "1.0.0",
    "private": true,
    "license": "UNLICENSED",
    "engines": {
        "node": ">=24",
        "npm": ">=11",
        "pnpm": ">=10"
    },
    "packageManager": "pnpm@10.24.0",
    "scripts": {
        "start": "pnpm exec ng serve",
        "start:http": "pnpm exec ng serve -c http",
        "build": "pnpm exec ng build",
        "build:dev": "pnpm exec ng build -c dev",
        "docker:build": "docker buildx bake -f .docker/docker-bake.hcl",
        "test": "pnpm exec vitest run",
        "test:dev": "pnpm exec vitest",
        "test:coverage-reports": "pnpm exec vite preview --outDir reports/dma-web-client",
        "e2e": "pnpm exec playwright test -c e2e/playwright.config.ts",
        "lint": "pnpm exec ng lint",
        "format:check": "pnpm exec prettier -l .",
        "format:write": "pnpm exec prettier -w ."
    },
    "dependencies": {
        "@angular/cdk": "~20.2.0",
        "@angular/common": "~20.3.0",
        "@angular/compiler": "~20.3.0",
        "@angular/core": "~20.3.0",
        "@angular/forms": "~20.3.0",
        "@angular/platform-browser": "~20.3.0",
        "@angular/router": "~20.3.0",
        "prettier": "~3.6.0",
        "rxjs": "~7.8.0",
        "tslib": "~2.8.0"
    },
    "devDependencies": {
        "@analogjs/platform": "~1.21.0",
        "@analogjs/vite-plugin-angular": "~1.21.0",
        "@analogjs/vitest-angular": "~1.21.0",
        "@angular/build": "~20.3.0",
        "@angular/cli": "~20.3.0",
        "@angular/compiler-cli": "~20.3.0",
        "@eslint/js": "~9.37.0",
        "@playwright/test": "~1.56.0",
        "@types/node": "~22.18.0",
        "@vitest/browser": "~3.2.0",
        "@vitest/coverage-v8": "~3.2.0",
        "@vitest/ui": "~3.2.0",
        "angular-eslint": "~20.3.0",
        "eslint": "~9.37.0",
        "eslint-config-prettier": "~10.1.0",
        "eslint-plugin-prettier": "~5.5.0",
        "playwright": "~1.56.0",
        "prettier-plugin-organize-imports": "~4.3.0",
        "typescript": "~5.9.0",
        "typescript-eslint": "~8.46.0",
        "vite-tsconfig-paths": "~5.1.0",
        "vitest": "~3.2.0"
    }
}
